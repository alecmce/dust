# Flash 9+
-main TestMain
-cp <%= get('flash', 'src') %>
-cp <%= get('testing', 'src') %>
-lib munit
<% get_list('flash', 'libs').each do |library| %>-lib <%= library %>
<% end%>-swf-version <%= get('flash', 'version') %>
-swf <%= get('testing', 'bin') %>/as3_test.swf

--next

# JavaScript
-main TestMain
-cp <%= get('html5', 'src') %>
-cp <%= get('testing', 'src') %>
-lib munit
<% get_list('js', 'libs').each do |library| %>-lib <%= library %>
<% end %>-js <%= get('testing', 'bin') %>/js_test.js

--next

## CPP
-main TestMain
-cp <%= get('cpp', 'src') %>
-cp <%= get('testing', 'src') %>
-lib munit
<% get_list('html5', 'libs').each do |library| %>-lib <%= library %>
<% end %><%= '-D HXCPP_M64' unless get('cpp', '64bit').nil? %>
-cpp <%= get('testing', 'bin') %>/cpp_test

--next

