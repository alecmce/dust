<% set_context('flash') %>
# Flash 9+
-main TestMain
-cp <%= testing.src %>
-cp <%= testing.test %>
-lib munit
<% libraries.each do |library| %>
-lib <%= library %>
<% end %>
-swf-version 9
-swf <%=bin%>/as3_test.swf

--next

#Flash 8
-main TestMain
-cp <%= src %>
-cp <%= test %>
-lib munit
<% libraries.each do |library| %>
    -lib <%= library %>
<% end %>
-swf-version 8
-swf <%=bin%>/as2_test.swf

--next

